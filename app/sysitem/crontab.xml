<cronentries>
    <cron id="sysitem_tasks_storeSyncDB">
        <description>将redis中改变库存的商品库存同步到数据中</description>
        <schedule>* * * * *</schedule>
        <enabled>true</enabled>
    </cron>
    <cron id="sysitem_tasks_syncToRedis">
        <description>将数据库中商品库存数据同步到redis中</description>
        <schedule>* * * * *</schedule>
        <enabled>true</enabled>
    </cron>
</cronentries>
